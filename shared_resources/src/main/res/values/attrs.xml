<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableView">
        <!-- State identifier indicating the group is expanded. -->
        <attr name="state_expanded" format="boolean" />
    </declare-styleable>

    <!-- Default style for slider preferences. -->
    <attr name="sliderPreferenceStyle" format="reference" />

    <declare-styleable name="SliderPreference">
        <attr format="float" name="sliderMin" />
        <attr format="float" name="sliderMax" />
        <attr format="float" name="sliderStepSize" />
        <attr name="android:layout" />
        <attr format="boolean" name="adjustable" />
        <attr format="boolean" name="showSliderValue" />
        <attr format="boolean" name="updatesContinuously" />
        <attr format="boolean" name="showSummary" />
    </declare-styleable>
</resources>